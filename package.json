{
  "name": "warm-up",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "pnpm msw:init",
    "dev": "next dev",
    "prebuild": "pnpm msw:clean",
    "build": "next build",
    "start": "next start",
    "pretest": "pnpm msw:init",
    "test": "jest --watch",
    "coverage": "pnpm pretest && jest --coverage",
    "format": "prettier \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,json,yaml,yml,xml,html,md,css}\" --write",
    "lint": "next lint --dir=src --ext={js,mjs,cjs,jsx,ts,mts,cts,tsx}",
    "postinstall": "husky install",
    "commit": "git-cz",
    "msw:init": "npx msw init ./public --save",
    "msw:clean": "rm ./public/mockServiceWorker.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@tanstack/react-query": "^4.29.4",
    "axios": "^1.4.0",
    "next": "13.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.5.0",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@emotion/eslint-plugin": "^11.10.0",
    "@tanstack/eslint-plugin-query": "^4.29.4",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jsdom": "*",
    "@types/node": "^18.0.0",
    "@types/react": "18.2.3",
    "@types/react-dom": "18.2.3",
    "@types/testing-library__jest-dom": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "commitizen": "^4.3.0",
    "eslint": "^8.39.0",
    "eslint-config-next": "13.3.4",
    "eslint-config-prettier": "^8.8.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
